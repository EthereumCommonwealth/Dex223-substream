{
  "name": "subgraphtest",
  "license": "UNLICENSED",
  "scripts": {
    "template": "export $(xargs < .env) && mustache ./config/$NETWORK.js subgraph.template.yaml > subgraph.yaml && mustache ./config/$NETWORK.js ./src/utils/constants.template.ts > ./src/utils/constants.ts",
    "codegen": "buf generate --exclude-path=\"sf/substreams\" --type=\"dex223.v1.Events\" ../substreams/dex223-v0.1.0.spkg#format=bin && graph codegen",
    "graph": "graph",
    "build": "graph build",
    "create-local": "export $(xargs < .env) && graph create --node http://localhost:8020/ dex223-v1-$NETWORK",
    "remove-local": "export $(xargs < .env) &&  graph remove --node http://localhost:8020/ dex223-v1-$NETWORK",
    "deploy-local": "export $(xargs < .env) && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 dex223-v1-$NETWORK",
    "pause-local": "docker exec graph-node graphman unassign subgraphtest",
    "resume-local": "docker exec graph-node graphman reassign subgraphtest default",
    "clean-local-data": "rm -rf ./graph-node/data",
    "launch-graph-node": "./graph-node/start.sh"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.86.0",
    "@graphprotocol/graph-ts": "^0.34.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "as-proto": "^1.3.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "protoc-gen-js": "^3.21.2",
    "protocol-buffers": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "as-proto-gen": "^1.3.0",
    "mustache": "^4.0.1"
  }
}
